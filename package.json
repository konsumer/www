{
  "name": "www",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm build && babel-node src/server.js",
    "build": "browserify src/index.js | uglifyjs > pub/bundle.js"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "sassify": "^0.10.0",
    "stringify": "^5.1.0",
    "uglify-js": "^2.7.0",
    "express": "^4.13.4"
  },
  "engines": { "node": "5.5.0", "npm": "3.10.5" },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "stringify",
        {
          "appliesTo": {
            "includeExtensions": [
              ".md",
              ".txt",
              ".htm",
              ".html"
            ]
          }
        }
      ],
      [
        "sassify",
        {
          "auto-inject": true
        }
      ]
    ]
  }
}
